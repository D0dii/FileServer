# Authentication
# The secret password for accessing the application
VITE_SECRET_PASSWORD=password

# API Configuration
# Base URL for the backend API server
VITE_API_BASE_URL=http://localhost:8080

# App Configuration
# Application title (optional)
VITE_APP_TITLE=File Server

# Maximum file size for uploads in MB (optional, for future use)
VITE_MAX_FILE_SIZE_MB=10

# Refresh Intervals (in milliseconds)
# How often to refresh the file list
VITE_FILES_REFRESH_INTERVAL=5000

# How often to refresh disk usage information
VITE_DISK_USAGE_REFRESH_INTERVAL=30000
